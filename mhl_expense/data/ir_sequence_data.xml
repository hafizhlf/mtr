<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <record id="sequence_hr_expense" model="ir.sequence">
            <field name="name">HR Expense Sequence</field>
            <field name="code">hr.expense</field>
            <field name="suffix">/EXP-MHL/%(rom_month)s/%(year)s</field>
            <field name="padding">4</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>
        
        <record id="sequence_hr_expense_report" model="ir.sequence">
            <field name="name">HR Expense Report Sequence</field>
            <field name="code">hr.expense.sheet</field>
            <field name="suffix">/PR-MHL/%(rom_month)s/%(year)s</field>
            <field name="padding">4</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>
        
        <record id="sequence_cash_advance_report" model="ir.sequence">
            <field name="name">Cash Advance Sequence</field>
            <field name="code">cash.advance</field>
            <field name="suffix">/CA/%(rom_month)s/%(year)s</field>
            <field name="padding">4</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>
    </data>
</odoo>
